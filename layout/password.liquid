<body class="template-password">

  {{ content_for_layout }}

  <div id="modal-login" class="password-page-modal-wrapper">
    <div class="password-page-modal">
      <div class="module header">
        <span class="close-modal">v</span>
        <p class="form-title">{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_heading' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_heading' | t }}{% endcapture %}{% include 'ly-static-string' with '9616588' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
      </div>

      {% form 'storefront_password' %}
        {{ form.errors | default_errors }}
        <div class="password-page-field-wrap" {% if form.errors %}data-open-modal{% endif %}>
          <input class="password-page-input" type="password" name="password" id="password" placeholder="{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_password_label' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_password_label' | t }}{% endcapture %}{% include 'ly-static-string' with '9616589' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}" autofocus>
          <input class="submit" name="submit" type="submit" value="{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_submit' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_submit' | t }}{% endcapture %}{% include 'ly-static-string' with '9616590' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
        </div>
      {% endform %}

      <p class="admin-login">
        {% comment %}ly_i18n_replace_for_[ 'general.password_page.admin_link_question' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.admin_link_question' | t }}{% endcapture %}{% include 'ly-static-string' with '9616591' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
        <a href="/admin" class="admin-link">{% comment %}ly_i18n_replace_for_[ 'general.password_page.admin_link_direction' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.admin_link_direction' | t }}{% endcapture %}{% include 'ly-static-string' with '9616592' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a>
      </p>
    </div>
  </div>

  <script
    type="application/json"
    data-scripts
  >
    {
      "countries": {{ "/services/javascripts/countries.js" | json }},
      "currencies": "//cdn.shopify.com/s/javascripts/currencies.js",
      "customer_area": {{ "customer_area.js"  | shopify_asset_url | json }},
      "jquery_api": {{ 'api.jquery.js' | shopify_asset_url | json }},
      "jquery_cart": {% comment %}ly_asset_replace_for_[ "jquery.cart.min.js" | asset_url | json ]_begin{% endcomment %}{% assign ly_asset = "jquery.cart.min.js" %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | json }}{% comment %}ly_asset_replace_end{% endcomment %},
      "jquery_currencies": {% comment %}ly_asset_replace_for_[ "jquery.currencies.js" | asset_url | json ]_begin{% endcomment %}{% assign ly_asset = "jquery.currencies.js" %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | json }}{% comment %}ly_asset_replace_end{% endcomment %},
      "plugins": {% comment %}ly_asset_replace_for_[ 'plugins.js' | asset_url | json ]_begin{% endcomment %}{% assign ly_asset = 'plugins.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | json }}{% comment %}ly_asset_replace_end{% endcomment %},
      "shopify_common": {{ "shopify_common.js" | shopify_asset_url | json }}
    }
  </script>

